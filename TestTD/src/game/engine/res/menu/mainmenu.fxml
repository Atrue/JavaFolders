<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?scenebuilder-stylesheet menustyle.css?>
<?import javafx.scene.layout.VBox?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>

<GridPane alignment="TOP_LEFT" hgap="10" minWidth="1024.0" prefWidth="1024.0" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="game.MainMenuController">
          <padding><Insets bottom="10" left="25" right="50" top="100" /></padding>
   <children>
   
       <Label fx:id="tdlabel" alignment="CENTER" maxWidth="1.7976931348623157E308" text="YxZ TD" GridPane.columnIndex="1" GridPane.rowIndex="1">
         <effect>
            <Lighting bumpInput="$null" diffuseConstant="1.49" specularConstant="0.86">
               <light>
                  <Light.Distant color="#2b00ff" />
               </light>
            </Lighting>
         </effect></Label>
       <VBox alignment="CENTER" spacing="10" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="5" GridPane.valignment="CENTER">
          <children>
              <Button fx:id="loadGameButton" disable="true" onAction="#continueGame" prefHeight="35" prefWidth="120" text="Continue" />
              <Button fx:id="newGameButton" alignment="CENTER" onAction="#startNewGame" prefHeight="35" prefWidth="120" text="New Game" />
              <Button fx:id="leaderboardButton" prefHeight="35" prefWidth="120" text="Leaderboard" />
              <Button fx:id="optionsButton" prefHeight="35" prefWidth="120" text="Options" />
              <Button fx:id="exitButton" onAction="#exitGame" prefHeight="35" prefWidth="120" text="Exit" />
          </children>
   
       </VBox>
   </children>
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints maxWidth="1.7976931348623157E308" minWidth="-Infinity" prefWidth="1024.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
</GridPane>
